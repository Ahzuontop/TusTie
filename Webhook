local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer or Players:GetPlayers()[1] -- dùng LocalPlayer nếu có
local url = "https://WebhookProxy.trananhyus.repl.co/send"

local data = {
    username = player.Name,
    userid = player.UserId,
    key = "DracoUsinghb", -- Thay bằng key bạn muốn log
    executor = identifyexecutor and identifyexecutor() or "Unknown"
}

local success, err = pcall(function()
    HttpService:PostAsync(url, HttpService:JSONEncode(data), Enum.HttpContentType.ApplicationJson)
end)

if not success then
    warn("Gửi webhook thất bại:", err)
end
